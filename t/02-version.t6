use Test;
use Test::When <author>;
use Docker::API;

plan 5;

isa-ok my $docker = Docker::API.new, Docker::API;

ok $docker.ping, 'ping';

ok my $version = $docker.version, 'Request Docker version';

ok $version<Version>, 'Version of docker';

ok $version<KernelVersion>, 'Kernel Version';

done-testing;
