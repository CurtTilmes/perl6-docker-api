use Test;
use Test::When <author>;
use Docker::API;

plan 6;

isa-ok my $docker = Docker::API.new, Docker::API;

ok my $info = $docker.info, 'Request Docker system information';

ok $info<ServerVersion>, 'ServerVersion';

ok $info<KernelVersion>, 'KernelVersion';

ok $info<OSType>, 'OSType';

ok $info<ProductLicense>, 'ProductLicense';

done-testing;
